!VUMeter methodsFor:'change & update'!

update:something with:aParameter from:changedObject
    "Invoked when an object that I depend upon sends a change notification."

    "stub code automatically generated - please change as required"

    changedObject == model ifTrue:[
        self invalidate.
        ^ self
    ].
    super update:something with:aParameter from:changedObject
! !

!VUMeter methodsFor:'drawing'!

redrawValue
    |v org rad scaleX scaleY x y|

    org := 75@84.
    scaleX := 55.
    scaleY := 50.

    v := model value ? 0.
    v := v clampBetween:0 and:1.

    "/ map 0..1 into 0..pi scaling and shifting the phase
    rad := v * (Float pi*0.8) - (Float pi*0.9).

    x := rad cos * scaleX.
    y := rad sin * scaleY.

    self lineWidth:2.
    self paint:(Color grey:50).
    self displayLineFrom:org to:org+(x@y).
    self lineWidth:1.
    self paint:(Color grey:20).
    self displayLineFrom:org to:org+(x@y).

    "
     |m v|

     m := 0 asValue.
     v := self new.
     v model:m.
     m inspect.
     v open.
    "
!

redrawX:x y:y width:w height:h
    "called to redraw a part of the widgets area. x/y define the origin, w/h the size of
     that area. The clipping region has already been set by the caller, so even if the code
     below draws outside the redraw-area, it will not affect what is on the screen. 
     Therefore, the example below can fill the rectangle in the redraw area, but still draw
     the cross in the outside regions."

    |img|

    isPowerOn ifTrue:[
        img := powerOnImage        
    ] ifFalse:[
        img := powerOffImage
    ].
    img displayOn:self x:0 y:0.
    self redrawValue.
! !

!VUMeter methodsFor:'initialization & release'!

initialize
    super initialize.

    isPowerOn isNil ifTrue:[ isPowerOn := true ].
    powerOnImage := self class onImage onDevice:device.
    powerOffImage := self class offImage onDevice:device.

    self viewBackground:(Color rgbValue:16r333433)

    "
     self new open
    "
! !

!VUMeter methodsFor:'queries'!

preferredExtent
    ^ powerOnImage extent
! !

