!KeyboardView methodsFor:'event handling'!

buttonPress:button x:x y:y
    |info keys bounds|

    info := self buttonAtX:x y:y.
    bounds := info first.
    keys := info second.
    self invertRectangle:bounds.
    lastKeyInfo := info
!

buttonRelease:button x:x y:y
    |info keys bounds key|

    info := lastKeyInfo.
    bounds := info first.
    keys := info second.
    self invertRectangle:bounds.

    targetView notNil ifTrue:[
        key := self shifted ifTrue:[ keys first ] ifFalse:[ keys second].
        targetView buttonPress:key x:0 y:0.
        targetView buttonRelease:key x:0 y:0.    
    ].

