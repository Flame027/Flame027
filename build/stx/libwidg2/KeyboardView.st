!KeyboardView methodsFor:'initialization'!

defineKeysFromSpec:spec
    |yRun xRun|

    keyPositions := Dictionary new.
    shiftedPositions := Dictionary new.

    yRun := 1.
    spec do:[:eachRowSpec |
        |h widths shifted normal |

        h := eachRowSpec first.
        widths := eachRowSpec second.
        shifted := eachRowSpec third.
        normal := eachRowSpec fourth.

        xRun := 1.
        1 to:normal size do:[:i |
            |w shKey key|

            w := widths at:i.
            shKey := shifted at:i.
            key := normal at:i.
            keyPositions at:{ key . shKey } put:(xRun@yRun extent:w@h).

            xRun := xRun + w + 1.
        ].
        yRun := yRun + h + 1.
    ].

