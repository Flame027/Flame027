!ChickenFun class methodsFor:'start & stop'!

isRunning
    ^ MyProcess notNil
!

start
    self stop.
    MyProcess := [
                    [
                        self new animationLoop
                    ] valueOnUnwindDo:[MyProcess := nil]
                 ] fork.
    MyProcess name:'ChickenFun'.
    MyProcess restartable:true.

    "
     self stop
     self start
    "
!

stop
    MyProcess notNil ifTrue:[
        MyProcess terminate.
        MyProcess := nil.
    ].

    "
     self stop
     self start
    "
! !

!ChickenFun methodsFor:'animation'!

animationLoop
    self getImages.
    imgWidth := lookImages inject:0 into:[:maxSoFar :thisImage | maxSoFar max:(thisImage width)].
    imgHeight := lookImages inject:0 into:[:maxSoFar :thisImage | maxSoFar max:(thisImage height)].
    savedBits := Form width:imgWidth height:imgHeight depth:Display depth.
    workBits := Form width:imgWidth height:imgHeight depth:Display depth.

    tick := 0.03.

    [true] whileTrue:[
        targetView := self selectAnyView.
        targetView notNil ifTrue:[
            self showImagesIn:targetView.
        ].
        self waitSomeTime.
    ]

    "
     self new start
    "
!

getImages
    lookImages := (1 to:14) collect:[:index | |img|
                                    img := self class perform:('look' , (index printStringLeftPaddedTo:2 with:$0)) asSymbol.
                                    img clearMaskedPixels.
                                    img := img onDevice:Display.
                                ].

    holeImages := (1 to:6) collect:[:index | |img|
                                    img := self class perform:('hole' , (index printString)) asSymbol.
                                    img clearMaskedPixels.
                                    img := img onDevice:Display.
                                ].

    "
     self new getImages
    "
!

selectAnyView
    |allUsableViews|

    allUsableViews := Display allViews select:[:eachView |
                                                eachView isRootView not
                                                and:[eachView shown
                                                and:[eachView topView realized 
                                                and:[eachView topView shown
                                                and:[eachView topView isPopUpView not]]]]
                                             ].
    "/ only those without subviews ...
    allUsableViews := allUsableViews select:[:eachView | eachView subViews size == 0].
    allUsableViews := allUsableViews select:[:eachView | eachView width > 40
                                                         and:[eachView height > 80]].
    allUsableViews := allUsableViews asOrderedCollection.
    ^ allUsableViews at:(Random nextIntegerBetween:1 and:allUsableViews size) ifAbsent:nil

!

showImage:anImage in:aView atX:x y:y forSeconds:time
    workBits copyFrom:savedBits x:0 y:0 toX:0 y:0 width:imgWidth height:imgHeight.
    workBits displayImage:anImage x:0 y:0.

    aView displayImage:workBits x:x y:y.
    aView device sync.
    Delay waitForSeconds:time.
!

showImagesIn:aView 
    |x y sensor|

    x := Random nextIntegerBetween:0 and:(aView width - imgWidth).
    y := Random nextIntegerBetween:0 and:(aView height - imgHeight).
    targetRectangle := (x@y) extent:(imgWidth@imgHeight).

    Processor activeProcess withPriority:10 do:[
        (sensor := aView sensor) notNil ifTrue:[
            sensor addEventListener:self
        ].
        aView class drawingOnClosedDrawableSignal handle:[:ex |
        ] do:[
            self showImagesIn:aView atX:x y:y
        ].
        sensor notNil ifTrue:[
            sensor removeEventListener:self
        ].
    ]
!

showImagesIn:aView atX:x y:y
    [
        |t|

        savedBits copyFrom:aView x:x y:y toX:0 y:0 width:imgWidth height:imgHeight.
        workBits copyFrom:savedBits x:0 y:0 toX:0 y:0 width:imgWidth height:imgHeight.

        gotcha := false.
        "/ hole opens ...
        1 to:5 do:[:imageIndex | 
            self showImage:(holeImages at:imageIndex) in:aView atX:x y:y forSeconds:tick.
        ].
        gotcha ~~ true ifTrue:[
            "/ entering ...
            1 to:8 do:[:imageIndex | 
                self showImage:(lookImages at:imageIndex) in:aView atX:x y:y forSeconds:(tick*3).
            ].
            gotcha ~~ true ifTrue:[
                "/ a quick look ...
                9 to:14 do:[:imageIndex | 
                    gotcha ~~ true ifTrue:[
                        self showImage:(lookImages at:imageIndex) in:aView atX:x y:y forSeconds:(tick*3).
                    ]
                ].
                gotcha ~~ true ifTrue:[
                    Random nextBoolean ifTrue:[
                        "/ a deeper look ...
                        10 to:14 do:[:imageIndex | 
                            gotcha ~~ true ifTrue:[
                                self showImage:(lookImages at:imageIndex) in:aView atX:x y:y forSeconds:(tick*6).
                            ]
                        ].
                    ].
                    gotcha ~~ true ifTrue:[
                        Random nextBoolean ifTrue:[
                           "/ another look ...
                            10 to:14 do:[:imageIndex | 
                            gotcha ~~ true ifTrue:[
                                    self showImage:(lookImages at:imageIndex) in:aView atX:x y:y forSeconds:(tick*9).
                                ].
                            ].
                        ].
                        gotcha ~~ true ifTrue:[
                            Random nextBoolean ifTrue:[
                               "/ another look ...
                                10 to:14 do:[:imageIndex | 
                                gotcha ~~ true ifTrue:[
                                        self showImage:(lookImages at:imageIndex) in:aView atX:x y:y forSeconds:(tick*9).
                                    ].
                                ].
                            ].
                        ].
                    ].
                ].
            ].
            "/ leaving ...
            t := tick*3.
            8 to:1 by:-1 do:[:imageIndex | 
                (gotcha == true) ifTrue:[t := tick/2].
                self showImage:(lookImages at:imageIndex) in:aView atX:x y:y forSeconds:t.
            ].
        ].
        "/ hole closes ...
        t := tick.
        5 to:1 by:-1 do:[:imageIndex | 
            (gotcha == true) ifTrue:[t := tick/3].
            self showImage:(holeImages at:imageIndex) in:aView atX:x y:y forSeconds:t.
        ].
    ] valueNowOrOnUnwindDo:[
        aView redrawX:x y:y width:imgWidth height:imgHeight.
        aView device sync.
    ]
!

waitSomeTime
    Delay waitForSeconds:(Random nextIntegerBetween:5 and:30).
! !

!ChickenFun methodsFor:'events'!

processEvent:anEvent
    anEvent isButtonPressEvent ifTrue:[
        anEvent view == targetView ifTrue:[
            (targetRectangle containsPoint:(anEvent x @ anEvent y)) ifTrue:[
                "/ Transcript showCR:'gotcha'.
                gotcha := true.
                ^ true.
            ]
        ]
    ].
    ^ false
! !

