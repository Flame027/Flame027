    v origin:0.0@0.0 corner:1.0@1.0.
    top addSubView:v.
    top extent:(v preferredExtent).
    top open.
! !

!ChickenEyes methodsFor:'animation'!

animationLoop
    self getBits.

    self comeInSequence.    

    [drawableId notNil] whileTrue:[
        self waitSomeTime.
        drawableId notNil ifTrue:[
            clicked == 2 ifTrue:[    "/ right button - go away for a while
                clicked := nil.
                self goAwayAndComeBackSequence.    
            ] ifFalse:[
                clicked == 1 ifTrue:[    "/ left button - pick
                    clicked := nil.
                    self pickSequence.    
                    self pickSequence.    
                    self pickSequence.    
                    self pickSequence.    
                    self goAwayAndComeBackSequence.    
                ]
            ].
            self lookAtCursor.
        ]
    ]

    "
    "

    "Modified: / 18.8.2000 / 22:47:48 / cg"
!

comeInSequence
    self showOpeningHoleImages.    
    self showChickComingImages.
    self showChickLookAroundImages.
!

goAwayAndComeBackSequence
    |delay|

    clicked := nil.
    self goAwaySequence.    
    delay := Random nextIntegerBetween:1 and:10.
    delay timesRepeat:[
        clicked == 2 ifFalse:[
            Delay waitForSeconds:1
        ]
    ].
    self comeInSequence.    
!

goAwaySequence
    self showChickGoingImages.    
    self showClosingHoleImages.    
    self clear.    
    device sync.
!

lookAtCursor
    |cursorPosition myPosition alpha p segment imageName image delayTime|

    drawableId isNil ifTrue:[^ self "closed"].

    cursorPosition := self device pointerPosition.
    myPosition := self device translatePoint:0@0 from:drawableId to:(device rootWindowId).
    myPosition := myPosition + (self extent // 2) - (0@10) "delta from image-center to eyes".

    delayTime := 0.

    p := myPosition - cursorPosition.
    (p x abs < 20 and:[p y abs - 10  < 25]) ifTrue:[
        image := lookFwdImage.
    ] ifFalse:[
        alpha := p angle.
        segment := (((alpha + 22) \\ 360) / 45) + 1.
        imageName := #('look_west' 'look_nw' 'look_north' 'look_ne' 'look_east' 'look_se' 'look_south' 'look_sw') at:segment asInteger.
        "/ sigh - only some step-images are available in more fine grain
        (segment > 4.5 and:[segment < 5]) ifTrue:[
            "/ split 135..180
            imageName := 'look_nne'
        ].
"/        Transcript showCR:alpha asInteger.
"/        Transcript showCR:segment.
        image := self class perform:imageName asSymbol.
    ].

    image ~~ lastImage ifTrue:[
        timeSinceLookingFwd := Timestamp now.
        timeWhenZwinker := timeSinceLookingFwd addSeconds:(Random nextIntegerBetween:2 and:5).
    ] ifFalse:[
        Timestamp now > timeWhenZwinker ifTrue:[
            ((p x abs < 20 and:[p y abs - 10  < 25]) 
            and:[ p y + 20 < 0 ]) ifTrue:[
                (Random nextIntegerBetween:1 and:10) > 8 ifTrue:[
                    self zwinkerSequence
                ] ifFalse:[
                    self pickSequence.
                ].
                timeWhenZwinker := Timestamp now addSeconds:(Random nextIntegerBetween:0.5 and:2).
            ] ifFalse:[
                image == lookFwdImage ifFalse:[
                    self showChickRandomLookAroundImages.
                ].
                timeWhenZwinker := Timestamp now addSeconds:(Random nextIntegerBetween:5 and:10).
            ].
            image == lookFwdImage ifTrue:[
                ^ self
            ]
        ] ifFalse:[
            ^ self
        ]
    ].

    self showImage:image forSeconds:delayTime.
    lastImage := image.
!

pickSequence
    self showImage:pickImage1 forSeconds:(tick*3).
    self showImage:pickImage2 forSeconds:(tick).
!

showChickComingImages
    |t|

    "/ entering ...
    1 to:8 do:[:imageIndex | 
        self showImage:(comeImages at:imageIndex) forSeconds:(tick*3).
    ].
!

showChickGoingImages
    |t|

    "/ leaving ...
    8 to:1 by:-1 do:[:imageIndex | 
        self showImage:(comeImages at:imageIndex) forSeconds:(tick*3).
    ].
!

showChickLookAroundImages
    "/ a quick look around ...
    9 to:14 do:[:imageIndex | 
        self showImage:(comeImages at:imageIndex) forSeconds:(tick*3).
    ].
!

showChickRandomLookAroundImages
    "random look around (done at random time intervals, if nothing happens with the mouse)"

    |sequence|

    Random nextBoolean ifTrue:[
        self zwinkerSequence.
    ].
    sequence := #(
                    ( #'look_fwd')
                    ( #'look_north' #'look_fwd' #'look_south' #'look_fwd')
                    ( #'look_west' #'look_fwd' #'look_east' #'look_fwd')
                    ( #'look_ne' #'look_fwd' #'look_sw' #'look_fwd')
                    ( #'look_se' #'look_fwd' #'look_nw' #'look_fwd')
                    ( #'look_south' #'look_se' #'look_east' #'look_ne' #'look_fwd')
                    ( #'look_north' #'look_nw' #'look_north' #'look_ne' #'look_north' #'look_fwd')
                    ( #'look_south' #'look_sw' #'look_south' #'look_se' #'look_south' #'look_fwd')
                    ( #'look_west' #'look_nw' #'look_west' #'look_sw' #'look_west' #'look_fwd')
                    ( #'look_east' #'look_ne' #'look_east' #'look_se' #'look_east' #'look_fwd')
                    ( #'look_fwd' #'look_fwd_zw1' #'look_fwd_zw2' #'look_fwd' #'look_fwd' #'look_fwd')
                    ( #'look_fwd' #'look_fwd_zw1' #'look_fwd_zw2' #'look_fwd' #'look_fwd' #'look_fwd')
                    ( #'look_fwd' #'look_fwd_zw1' #'look_fwd_zw2' #'look_fwd' #'look_fwd' #'look_fwd')
                    ( #'look_fwd' #'look_fwd_zw1' #'look_fwd_zw2' #'look_fwd' #'look_fwd' #'look_fwd')
                    ( #'look_fwd' #'look_fwd_zw1' #'look_fwd_zw2' #'look_fwd' #'look_fwd' #'look_fwd')
                ) at:(Random nextIntegerBetween:1 and:15).
    sequence do:[:nm | 
        self showImage:(lookImages at:nm) forSeconds:(tick*9).
    ].

    Random nextBoolean ifTrue:[
        self zwinkerSequence.
    ].

    "Modified: / 18.8.2000 / 19:54:22 / cg"
!

showClosingHoleImages 
    |t|

    "/ hole closes ...
    t := tick.
    5 to:1 by:-1 do:[:imageIndex | 
        self showImage:(holeImages at:imageIndex) forSeconds:t.
    ].
!

showImage:anImage forSeconds:time
    |x y|

    self isOpen ifFalse:[^ self].

    x := (self width - imgWidth) // 2.
    y := (self height - imgHeight) // 2.

    true "double buffered" ifTrue:[
        workBits copyFrom:savedBits x:0 y:0 toX:0 y:0 width:imgWidth height:imgHeight.
        workBits displayImage:anImage x:0 y:0.

        self displayImage:workBits x:x y:y.
    ] ifFalse:[
        self clearRectangleX:x y:y width:imgWidth height:imgHeight.
        self displayImage:anImage x:x y:y.
    ].
    self device sync.
    Delay waitForSeconds:time.
!

showOpeningHoleImages
    |t|

    "/ hole opens ...
    1 to:5 do:[:imageIndex | 
        self showImage:(holeImages at:imageIndex) forSeconds:tick.
    ].
!

waitSomeTime
    Delay waitForSeconds:0.1.
!

zwinkerSequence
    self showImage:zwImage1 forSeconds:0.1.
    self showImage:zwImage2 forSeconds:0.1.
    self showImage:lookFwdImage forSeconds:0.1.
! !

!ChickenEyes methodsFor:'event handling'!

buttonPress:button x:x y:y
    |center p|

    button == 1 ifTrue:[
        center := self extent // 2.
        p := (x@y) - center.
        (p x abs < 20 and:[p y abs < 25]) ifFalse:[
            "/ ignore, if not clicked in the chicken
            ^ self
        ].
    ].
    clicked := button
!

mapped
    super mapped.

    self clear.
    self getBits.
!

sizeChanged:how
    realized ifTrue:[
        self clear.
        lastImage := nil. "/ force redraw
        self getBits.
    ].
    super sizeChanged:how
! !

!ChickenEyes methodsFor:'images'!

getImage:selector
    |img|

    img := self class perform:selector.
    img clearMaskedPixels.
    img := img onDevice:device.
    ^ img
!

getImages
    holeImages := (1 to:6) collect:[:index | self getImage:('hole' , (index printString)) asSymbol. ].
    comeImages := (1 to:14) collect:[:index | self getImage:('come' , (index printStringLeftPaddedTo:2 with:$0)) asSymbol. ].

    lookImages := Dictionary new.
    #(#'look_fwd' #'look_north' #'look_ne' #'look_east' #'look_se' #'look_south' #'look_sw' #'look_west' #'look_nw' #'look_nne')
    do:[:nm | 
        lookImages at:nm put:(self getImage:nm asSymbol).
    ].

    lookImages at:#look_fwd put:(lookFwdImage := self getImage:#look_fwd).
    lookImages at:#look_fwd_zw1 put:(zwImage1 := self getImage:#look_fwd_zw1).
    lookImages at:#look_fwd_zw2 put:(zwImage2 := self getImage:#look_fwd_zw2).
    lookImages at:#look_pick_1 put:(pickImage1 := self getImage:#look_pick_1).
    lookImages at:#look_pick_2 put:(pickImage2 := self getImage:#look_pick_2).

    imgWidth := lookImages inject:0 into:[:maxSoFar :thisImage | maxSoFar max:(thisImage width)].
    imgWidth := holeImages inject:imgWidth into:[:maxSoFar :thisImage | maxSoFar max:(thisImage width)].
    imgHeight := lookImages inject:0 into:[:maxSoFar :thisImage | maxSoFar max:(thisImage height)].
    imgHeight := holeImages inject:imgHeight into:[:maxSoFar :thisImage | maxSoFar max:(thisImage height)].
    imgHeight := imgHeight max:(lookFwdImage height).
    imgHeight := imgHeight max:(zwImage1 height).
    imgHeight := imgHeight max:(zwImage2 height).
    imgHeight := imgHeight max:(pickImage1 height).
    imgHeight := imgHeight max:(pickImage2 height).

    "
     self new getImages; inspect
    "

