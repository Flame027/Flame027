! !

!ChickenEyes methodsFor:'initialization & release'!

fetchDeviceResources
    |bg|

    holeImages isNil ifTrue:[
        self getImages.

        tick := 0.03.

        bg := Smalltalk bitmapFromFileNamed:'wood2V.tiff' inPackage:'stx:libwidg3'.
        bg := bg mixed:4 with:Color white.
        self viewBackground:bg.

        savedBits := Form width:imgWidth height:imgHeight depth:device depth onDevice:device.
        workBits := Form width:imgWidth height:imgHeight depth:device depth onDevice:device.
    ].
    super fetchDeviceResources
!

recreate
    "recreate after a snapin or migration"

    holeImages := savedBits := workBits := nil.
    self fetchDeviceResources.
    super recreate.
!

reinitialize
    "reinit after snapIn"

    myProcess := nil.
    super reinitialize.
! !

!ChickenEyes methodsFor:'queries'!

preferredExtent
    imgWidth isNil ifTrue:[
        self fetchDeviceResources
    ].
    ^ (imgWidth * 3) @ (imgHeight * 1.5) rounded
! !

!ChickenEyes methodsFor:'redrawing'!

getBits
    |x y|

    x := (self width - imgWidth) // 2.
    y := (self height - imgHeight) // 2.
    savedBits copyFrom:self x:x y:y toX:0 y:0 width:imgWidth height:imgHeight.
!

redraw
    myProcess isNil ifTrue:[
        myProcess := [
            [
                self animationLoop
            ] valueNowOrOnUnwindDo:[
                myProcess := nil
            ]
        ] fork.
    ] ifFalse:[
        lastImage := nil "/ force redraw
    ]

    "
     self new open
    "
! !

